@*@page "{searchString?}"*@
@model IEnumerable<Bangazon.Models.Product>
@{
    ViewData["Title"] = "Home Page";
}

<h2>Recent Products</h2>
        

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a asp-controller="Products" asp-action="Details" asp-route-id="@item.ProductId">@item.Title</a>
                    
                </td>
            </tr>
        }
    </tbody>
</table>